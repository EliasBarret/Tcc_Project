	<parameter name="codVenda" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[SELECT banco.`Pessoa`.nome,
	banco.`ItemVenda`.quantidade,
	banco.`ItemVenda`.`valorParcial`,
	banco.`Venda`.`valorTotal`,
	banco.`Venda`.horario,
	banco.`Venda`.codigo,
	banco.`Produto`.descricao,
	banco.`Produto`.preco
FROM banco.`ItemVenda`
	INNER JOIN banco.`Venda` ON 
	 banco.`ItemVenda`.venda_codigo = banco.`Venda`.codigo 
	INNER JOIN banco.`Cliente` ON 
	 banco.`Venda`.cliente_codigo = banco.`Cliente`.codigo 
	INNER JOIN banco.`Pessoa` ON 
	 banco.`Cliente`.pessoa_codigo = banco.`Pessoa`.codigo 
	INNER JOIN banco.`Produto` ON 
	 banco.`ItemVenda`.produto_codigo = banco.`Produto`.codigo 
WHERE 
	 banco.`Venda`.codigo = $P{codVenda}]]>
	</queryString>
