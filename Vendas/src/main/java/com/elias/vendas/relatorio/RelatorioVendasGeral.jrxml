	<parameter name="CodVenda" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT venda.valorTotal AS venda_valorTotal,
	pessoa.nome AS pessoa_nome,
	produto.descricao AS produto_descricao,
	itemvenda.valorParcial AS itemvenda_valorParcial,
	itemvenda.quantidade AS itemvenda_quantidade,
	venda.codigo AS venda_codigo
FROM cliente cliente
	INNER JOIN venda venda ON 
	 cliente.codigo = venda.cliente_codigo 
	INNER JOIN pessoa pessoa ON 
	 cliente.pessoa_codigo = pessoa.codigo 
	INNER JOIN itemvenda itemvenda ON 
	 venda.codigo = itemvenda.venda_codigo 
	INNER JOIN produto produto ON 
	 itemvenda.produto_codigo = produto.codigo 
WHERE 
	 venda.codigo = $P{CodVenda}]]>
	</queryString>
