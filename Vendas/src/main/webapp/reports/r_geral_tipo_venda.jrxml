	<queryString language="SQL">
		<![CDATA[SELECT banco.`TipoDeVenda`.venda_codigo,
	banco.`Venda`.`valorTotal`,
	banco.`TipoDeVenda`.tipo,
	banco.`Venda`.horario
FROM banco.`TipoDeVenda`
	INNER JOIN banco.`Venda` ON 
	 banco.`TipoDeVenda`.venda_codigo = banco.`Venda`.codigo 
	 AND banco.`TipoDeVenda`.venda_codigo = banco.`Venda`.codigo
WHERE 
	banco.`TipoDeVenda`.tipo =  $P{TIPOVENDA}]]>
	</queryString>
